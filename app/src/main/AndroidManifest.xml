<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.naufalrafizi.dewarumah">

    <uses-permission android:name="android.permission.SYSTEM_ALERT_WINDOW" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".BeforeLogin.Activity.MainActivity"
            android:label="@string/app_name"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme.NoActionBar"
            android:windowSoftInputMode="stateHidden|adjustResize">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".BeforeLogin.Activity.Login"
            android:screenOrientation="portrait"
            android:windowSoftInputMode="stateHidden|adjustResize" />
        <activity
            android:name=".BeforeLogin.Activity.SignUp"
            android:screenOrientation="portrait"
            android:windowSoftInputMode="stateHidden|adjustResize" />
        <activity
            android:name=".AfterLogin.GM.DataGM.Activity.DataGMActivity"
            android:screenOrientation="portrait"
            android:windowSoftInputMode="stateHidden|adjustResize" />
        <activity
            android:name=".AfterLogin.SA.MainActivitySAgent"
            android:screenOrientation="portrait"
            android:windowSoftInputMode="stateHidden|adjustResize" />
        <activity android:name=".AfterLogin.SC.MainActivitySConsultan" />
        <activity android:name=".AfterLogin.GM.MainActivityGM" />
        <activity android:name=".AfterLogin.SA.TokoSA.Activity.TokoActivitySA" />
        <activity
            android:name=".AfterLogin.SA.ProspekSA.Activity.ProspekActivitySA"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".AfterLogin.SA.ProfilSA.ProfilActivitySA" />
        <activity android:name=".AfterLogin.SA.InputProspekSA.Activity.InputProspekSA" />
        <activity android:name=".AfterLogin.SA.BonusSA.Activity.BonusActivitySA" />
        <activity android:name=".AfterLogin.SC.DataAgentSC.Activity.DataAgentSC" />
        <activity android:name=".AfterLogin.SC.DataAgentSC.Activity.TimeLineSC" />
    </application>

</manifest>